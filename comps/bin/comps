#!/bin/sh

if [ "$#" -eq 0 ]; then
    echo "Usage"
    exit 1
fi

case "$1" in
      r)
          BIN="comps_resistor.exe" ;;
      g)
          BIN="comps_logicgates.exe" ;;
      *)
          echo "*** Error: Invalid argument"
          exit 1
esac

shift
exec $(dirname "$0")/"$BIN" $*
exit 0
